<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="vertical" android:layout_width="match_parent"    android:layout_height="match_parent">    <EditText        android:id="@+id/name"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/textView3"        android:layout_alignParentStart="true"        android:layout_marginTop="5dp"        android:ems="10"        android:inputType="textPersonName" />    <TextView        android:id="@+id/textView3"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentTop="true"        android:layout_centerHorizontal="true"        android:layout_marginTop="120dp"        android:text="Name of Story Board"        android:textSize="18sp" />    <Button        android:id="@+id/enter"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentTop="true"        android:layout_centerHorizontal="true"        android:layout_marginTop="219dp"        android:text="create" /></RelativeLayout>